<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.collegeadminapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.CollegeAdminApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".DeleteNoticeActivity"></activity>
        <activity android:name=".faculty.UpdateInfoActivity" />
        <activity android:name=".faculty.AddTeacherActivity" />
        <activity android:name=".faculty.UpdateFacultyActivity" />
        <activity android:name=".PdfActivity" />
        <activity android:name=".ImageActivity" />
        <activity android:name=".NoticeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>